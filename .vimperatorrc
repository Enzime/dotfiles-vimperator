set gui=none,tabs
" set defsearch=!g

noremap b B
noremap c b
noremap i c

noremap A H
noremap S F
noremap F L

map <C-a> gT
noremap <C-s> <C-u>
map <C-l> gt

map <C-h> gT
map <C-f> gt

noremap a <Nop>
